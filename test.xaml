        <ComboBox Name="comboBox2" HorizontalAlignment="Left" Margin="115,70,0,0" VerticalAlignment="Top" Width="55"/>
        <ComboBox Name="comboBox3" HorizontalAlignment="Left" Margin="190,70,0,0" VerticalAlignment="Top" Width="65"/>
        <TextBlock Name="textBlock4" HorizontalAlignment="Left" Margin="30,155,0,0" VerticalAlignment="Top" TextWrapping="Wrap" Text="CCSSM"/>
        <ComboBox Name="comboBox4" HorizontalAlignment="Left" Margin="115,155,0,0" VerticalAlignment="Top" Width="55"/>
        <ComboBox Name="comboBox5" HorizontalAlignment="Left" Margin="190,155,0,0" VerticalAlignment="Top" Width="65"/>
        
        <ComboBox Name="comboBox6" HorizontalAlignment="Left" Margin="275,70,0,0" VerticalAlignment="Top" Width="40" RenderTransformOrigin="0.597,-1.193"/>
