            <ComboBoxItem>SM</ComboBoxItem>
            <ComboBoxItem>STAMS</ComboBoxItem>
            <ComboBoxItem>CCSSR</ComboBoxItem>
            <ComboBoxItem>FR</ComboBoxItem>
            <ComboBoxItem>LF</ComboBoxItem>
            <ComboBoxItem>VF</ComboBoxItem>
            <ComboBoxItem>SV</ComboBoxItem>
            <ComboBoxItem>PH</ComboBoxItem>
            <ComboBoxItem>STARS</ComboBoxItem>
